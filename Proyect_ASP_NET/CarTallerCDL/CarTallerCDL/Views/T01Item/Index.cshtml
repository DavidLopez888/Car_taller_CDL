@model IEnumerable<CarTallerCDL.T01Item>

@{
    ViewData["Title"] = "Items";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Id item
